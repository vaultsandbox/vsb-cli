# VaultSandbox Gateway Configuration
# Copy this file to .env and fill in your actual values

# Gateway URL (default: http://localhost:3000)
VAULTSANDBOX_URL=http://localhost:3000

# Your VaultSandbox API key
VAULTSANDBOX_API_KEY=your-api-key-here

# SMTP server for sending test emails directly (same as VAULTSANDBOX_URL without the protocol)
SMTP_HOST=localhost
SMTP_PORT=25